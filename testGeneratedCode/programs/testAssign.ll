addAssignIntInt(x:int, y:int): int
{
    i: int = x;
    i += y;

    return i;
}

addAssignIntDouble(x:int, y:double): double
{
    i: double = x;
    i += y;

    return i;
}

addAssignDoubleInt(x:double, y:int): double
{
    i: double = x;
    i += y;

    return i;
}

addAssignDoubleDouble(x:double, y:double): double
{
    i: double = x;
    i += y;

    return i;
}

